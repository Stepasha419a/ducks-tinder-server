{
  "name": "stepasha419a-ducks-tinder-server-chat-service",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "ncc build ./src/main.ts -o dist --minify -t",
    "format": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "gen:proto": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto.cmd --ts_proto_opt=outputEncodeMethods=false --ts_proto_opt=returnObservable=true --ts_proto_opt=outputJsonMethods=false --ts_proto_opt=outputClientImpl=false --ts_proto_opt=lowerCaseServiceMethods=true --ts_proto_opt=outputIndex=true --ts_proto_out=./src/infrastructure/grpc/gen --ts_proto_opt=fileSuffix=.pb --proto_path=./proto ./proto/*.proto",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:dev:docker": "cross-env NODE_ENV=dev-docker nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint ./src/**/*{.ts,.tsx,.js,.jsx}",
    "lint:fix": "eslint ./src/**/*{.ts,.tsx,.js,.jsx} --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.config.e2e.js",
    "test:unit": "jest --config ./jest.config.unit.js",
    "test:unit-refactoring": "jest --config ./jest.config.unit-refactoring.js"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^7.1.1",
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/common": "^11.1.10",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.10",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^11.1.10",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/platform-socket.io": "^11.1.10",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/websockets": "^11.1.10",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "express": "^5.2.1",
    "joi": "^18.0.2",
    "pg": "^8.17.2",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.10",
    "@types/amqplib": "^0.10.8",
    "@types/jest": "30.0.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "@vercel/ncc": "^0.38.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "30.2.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.7.4",
    "prisma": "^6.19.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.10.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.9.3"
  }
}
