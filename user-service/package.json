  "scripts": {
    "prebuild": "rimraf dist",
    "build": "ncc build ./src/main.ts -o dist --minify -t",
    "format": "prettier --check \"src/**/*.ts\"",
    "format:fix": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:microservices": "concurrently \"npm:start:dev user\"",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:dev:docker": "cross-env NODE_ENV=dev-docker nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint ./src/**/*{.ts,.tsx,.js,.jsx}",
    "lint:fix": "eslint ./src/**/*{.ts,.tsx,.js,.jsx} --fix",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.config.e2e.js",
    "test:unit": "jest --config ./jest.config.unit.js",
    "test:unit-refactoring": "jest --config ./jest.config.unit-refactoring.js"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^6.0.2",
